<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RealEstateApp.FlashLight">
    <ContentPage.Content>
        <StackLayout>
            <ImageButton IsOpaque="True" Source="powerOn.png" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand"
                         HeightRequest="300" WidthRequest="300"
                         x:Name="btnFlashLightSwitch" Clicked="btnFlashLightSwitch_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>